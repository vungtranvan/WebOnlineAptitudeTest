   <!-- Signup modal content -->
<div id="changePassModal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">

            <div class="modal-body">
                <div class="text-center mt-2 mb-4">
                    <a class="text-success">
                        <span>
                            <img class="mr-2" src="/Content/assets/backend/plugins//assets/images/logo-icon.png"
                                 alt="" height="18"><img src="/Content/assets/backend/plugins/assets/images/logo-text.png" alt=""
                                                         height="18">
                        </span>
                    </a>
                    <p class="mt-3">Change PassWord</p>
                </div>

                <form class="pl-3 pr-3" id="FormChangePass">
                    <input type="hidden" id="userNameAdmin" name="userNameAdmin" value="@Session["UserAdmin"]" />

                    <div class="form-group">
                        <label for="emailaddress">Password Old</label>
                        <input class="form-control" type="password" id="passOld" name="passOld" placeholder="Enter your password old">
                    </div>

                    <div class="form-group">
                        <label for="password">Password New</label>
                        <input class="form-control" type="password" id="passNew" name="passNew" placeholder="Enter your password new">
                    </div>

                    <div class="form-group">
                        <label for="password">Confirm Password New</label>
                        <input class="form-control" type="password" id="passNewConfirm" name="passNewConfirm" placeholder="Re-enter your password new">
                    </div>

                    <div class="form-group text-center">
                        <a class="btn btn-danger btnCancleChangePass">
                            Cancle
                        </a>
                        <a class="btn btn-primary btnSaveChangePass">
                            Save Change
                        </a>
                    </div>

                </form>

            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->